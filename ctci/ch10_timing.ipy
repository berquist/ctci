print('insertion')
%timeit rc = r.copy(); insertion_sort(r)
print('insertion2')
%timeit rc = r.copy(); insertion2_sort(r)
print('insertion3')
%timeit rc = r.copy(); insertion3_sort(r)
print('insertion4')
%timeit rc = r.copy(); insertion3_sort(r)
